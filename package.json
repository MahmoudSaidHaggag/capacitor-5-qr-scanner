{
  "name": "@diningcity/capacitor-qr-scanner",
  "version": "0.0.7",
  "description": "A Capacitor plug in to scan QR Codes",
  "main": "dist/plugin.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "lint": "npm run prettier -- --check && npm run swiftlint -- lint",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "build": "npm run clean && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "DiningCity Group",
  "license": "MIT",
  "devDependencies": {
    "@capacitor/android": "^3.1.1",
    "@capacitor/core": "^3.1.1",
    "@capacitor/ios": "^3.1.1",
    "@ionic/prettier-config": "^1.0.0",
    "@ionic/swiftlint-config": "^1.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "prettier": "^2.0.5",
    "prettier-plugin-java": "^0.8.0",
    "rimraf": "^3.0.0",
    "rollup": "^2.21.0",
    "swiftlint": "^1.0.1",
    "typescript": "~3.8.3"
  },
  "peerDependencies": {
    "@capacitor/core": "^2.4.5"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "DiningcityCapacitorQrScanner.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/diningcity-group/capacitor-qr-scanner"
  },
  "bugs": {
    "url": "https://github.com/diningcity-group/capacitor-qr-scanner/issues"
  },
  "dependencies": {
    "@capacitor/cli": "^3.1.1",
    "@diningcity/capacitor-qr-scanner": "^0.0.6",
    "plugin": "^0.3.3"
  }
}
